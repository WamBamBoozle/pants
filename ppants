  --no-compile-scala-warnings \
  --test-junit-jvmargs='-DMVN_CI=1' \
  --test-junit-jvmargs='-DCI_RUN=1' \
  --test-junit-jvmargs='-DSBT_CI=1' \
  --test-junit-jvmargs='-DSKIP_FLAKY=1' \
  --test-junit-xmlreport \
  --test-junit-parallel-threads=1 \
  --test-junit-fail-fast \
  --test-junit-coverage-processor=cobertura \
  --test-junit-coverage \
  --test-junit-coverage-breakpoints-XXX \
  --test-junit-coverage-run-instrument-XXX \
  --test-junit-coverage-instrument-use-file-XXX \
  --test-junit-coverage-run-run-XXX \
  --test-junit-coverage-run-report-XXX \
  \
  curator/matcher:tests \
  -ldebug
  --test-junit-coverage-patterns='com.twitter.curator.*' \
